drop table members;
create table members(
num number primary key,
id varchar2(50) not null unique,
password varchar2(50) not null,
name varchar2(50) not null,
regdate date default sysdate,
tel1 varchar2(20) not null,
tel2 varchar2(20) not null,
tel3 varchar2(20) not null
);

drop sequence members_seq;
create sequence members_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
  
drop table faq;
create table faq(
   num number not null primary key,
   writer varchar2(20) not null,
   subject varchar2(100) not null,
   division varchar2(30) not null,
   reg_date date default sysdate not null,
   content varchar2(1000) not null
);
drop sequence faq_seq;
create sequence faq_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;

  drop table notice;
create table notice( 
   num number not null primary key,
   writer varchar2(20) not null,
   subject varchar2(100) not null,
   division varchar2(30) not null,
   reg_date date not null,
   content varchar2(1000) not null
);
drop sequence notice_seq;
create sequence notice_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
  
drop table board;
create table board( 
   num number not null primary key,
   writer varchar2(10) not null,
   subject varchar2(20) not null,
   reg_date date not null,
   readcount int default 0,
   ref number not null,
   re_step number not null,
   re_level number not null,
   content varchar2(1000) not null
);
drop sequence board_seq;
create sequence board_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
  
drop table reply;
create table reply(
rnum int primary key,
bnum int not null,
writer varchar2(10),
content varchar2(1000),
reg_date date default sysdate
);

drop sequence reply_seq;
create sequence reply_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
  
   drop sequence coupon_seq;
create sequence coupon_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;

drop table coupon;
create table coupon(
cp_num int primary key,
cp_id varchar2(20),
cp_name varchar2(50), 
cp_number varchar2(50) unique, 
cp_discount float,  
cp_regdate date,  
cp_duedate date,
cp_usedate date, 
cp_check varchar2(10) default 'false' 
);

drop sequence watch_seq;
create sequence watch_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
  
drop table watch;
create table watch(
num number not null,
movie_num number not null,
user_id varchar2(30) not null,
reg_date date
);

drop sequence jjim_seq;
create sequence jjim_seq
  increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;

drop table jjim;
create table jjim(
num number not null,
movie_num number not null,
user_id varchar2(30) not null
);

drop sequence commodity_seq;
create sequence commodity_seq
increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;


drop table commodity;
create table commodity(
num number primary key,
name varchar2(50) not null,
content varchar2(100) not null,
price number not null
);

drop sequence orders_seq;
create sequence orders_seq
increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;

drop table orders;
create table orders(
	onum number primary key, -- 상품번호
	mnum number references members(num) on delete set null, -- 멤버 외래키
	orderdate date default sysdate -- 오늘 날자
);
select * from orders;

drop sequence orderdetails_seq;
create sequence orderdetails_seq
increment by 1
  start with 1
  minvalue 1
  maxvalue 10000
  nocache;
select * from orderdetails;

drop table orderdetails;
create table orderdetails(
  odid number primary key, 
  oid number references orders(onum) on delete cascade, -- 상품번호
  pnum number references commodity(num) on delete set null
  
