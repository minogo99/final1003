<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<typeAliases>
		<typeAlias type="member.model.MemberBean" alias="MyMember"/>
		<typeAlias  type="cs.model.CsNoticeBean" alias="MyCsNotice"/>
		<typeAlias  type="cs.model.CsFaqBean" alias="MyCsFaq"/>
		<typeAlias type="board.model.BoardBean" alias="MyBoard"/>
		<typeAlias type="category.model.CategoryBean" alias="MyCategory"/>
		<typeAlias type="board.model.ReplyBean" alias="MyReply"/>
		<typeAlias  type="admin.model.AdminDTO" alias="MyAdmin"/>
		<typeAlias type="category.detail.DetailBean" alias="MyC_Detail"/>
		<typeAlias type="main.model.MainBean" alias="MyMain"/>
		<typeAlias type="category.detail.DetailMainBean" alias="MyMainDetail"/>
	</typeAliases>
	<mappers>
	<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/notice.xml" />
		<mapper resource="mybatis/faq.xml" />
		<mapper resource="mybatis/board.xml" />
		<mapper resource="mybatis/category.xml" />
		<mapper resource="mybatis/reply.xml" />
		<mapper resource="mybatis/admin.xml" />
		<mapper resource="mybatis/detail.xml" />
		<mapper resource="mybatis/main.xml" />
		<mapper resource="mybatis/maindetail.xml" />
	</mappers>
</configuration>