<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="order.model.OrderBean">
	<insert id="insertOne">
		insert into orders values(orders_seq.nextval,#{mnum},sysdate)
	</insert>
	<select id="selectOne" resultType="MyOrder">
		select * from orders where mnum=#{mnum}
	</select>
	<insert id="setInsertDetail" >
		insert into orderdetails values(orderdetails_seq.nextval,#{onum},#{cnum})
	</insert>
</mapper>